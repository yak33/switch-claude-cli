{
  "name": "switch-claude-cli",
  "version": "1.3.3",
  "description": "Smart Claude API provider switcher with availability detection and caching",
  "main": "dist/index.js",
  "bin": {
    "switch-claude": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "tsx src/index.ts",
    "dev:watch": "tsx watch src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "type-check": "tsc --noEmit",
    "pretest": "npm run type-check && npm run lint",
    "prebuild": "npm run type-check",
    "prepublishOnly": "npm run clean && npm run build",
    "start": "node dist/index.js",
    "clean": "rimraf dist",
    "publish:beta": "npm version prerelease --preid=beta --no-git-tag-version && npm publish --tag beta",
    "publish:alpha": "npm version prerelease --preid=alpha --no-git-tag-version && npm publish --tag alpha",
    "publish:typescript": "npm version prerelease --preid=typescript --no-git-tag-version && npm publish --tag typescript"
  },
  "keywords": [
    "claude",
    "api",
    "cli",
    "switcher",
    "anthropic",
    "ai",
    "provider",
    "third-party"
  ],
  "author": "yak33 <vuanne1@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yak33/switch-claude-cli.git"
  },
  "bugs": {
    "url": "https://github.com/yak33/switch-claude-cli/issues"
  },
  "homepage": "https://github.com/yak33/switch-claude-cli#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "inquirer": "^9.0.0",
    "update-notifier": "^7.3.1"
  },
  "type": "module",
  "files": [
    "dist/",
    "providers.example.json",
    "README.md",
    "README.*.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^24.5.2",
    "@types/update-notifier": "^6.0.8",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "c8": "^10.1.3",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
