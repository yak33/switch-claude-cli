{
  "name": "switch-claude-cli",
  "version": "1.3.0",
  "description": "Smart Claude API provider switcher with availability detection and caching",
  "main": "index.js",
  "bin": {
    "switch-claude": "./index.js"
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "pretest": "npm run lint",
    "start": "node index.js"
  },
  "keywords": [
    "claude",
    "api",
    "cli",
    "switcher",
    "anthropic",
    "ai",
    "provider",
    "third-party"
  ],
  "author": "yak33 <vuanne1@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yak33/switch-claude-cli.git"
  },
  "bugs": {
    "url": "https://github.com/yak33/switch-claude-cli/issues"
  },
  "homepage": "https://github.com/yak33/switch-claude-cli#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "inquirer": "^9.0.0",
    "update-notifier": "^7.3.1"
  },
  "type": "module",
  "files": [
    "index.js",
    "providers.example.json",
    "README.md",
    "SECURITY.md"
  ],
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "c8": "^10.1.3",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "vitest": "^3.2.4"
  }
}
